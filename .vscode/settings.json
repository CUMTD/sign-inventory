{
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"cSpell.words": ["clsx", "dtid", "Gtfs"],
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"files.exclude": {
		"**/.DS_Store": true,
		"**/.git": true,
		"**/.hg": true,
		"**/.next": true,
		"**/.svn": true,
		"**/CVS": true,
		"**/Thumbs.db": true,
		"**/node_modules": true,
		"**/yarn.lock": true
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	}
}
